{
	"manifest_version": 2,
	"name": "Cybozu Plus (Unofficial)",
	"short_name": "Cybozu+",
	"version": "0.1.1",
	"icons": {
		"16" : "icon16.png",
		"48" : "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["jquery-2.1.4.min.js", "content-cybozu.js"]
		}
	],
	"permissions": [
		"http://*/*",
		"https://*/*"
	]
}
